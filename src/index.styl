.material-icons, .mi
  font-family: 'Material Icons'
  font-weight: normal
  font-style: normal
  display: inline-flex
  vertical-align: middle
  font-size: 24px
  line-height: 1
  letter-spacing: normal
  text-transform: none
  // display: inline-block
  white-space: nowrap
  word-wrap: normal
  direction: ltr
  -webkit-font-feature-settings: 'liga'
  -webkit-font-smoothing: antialiased

