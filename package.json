{
  "name": "n7zip_monitor",
  "version": "0.1.0",
  "description": "vite app",
  "private": true,
  "main": "./build/background.js",
  "scripts": {
    "serve": "vite -c config/vite.config.ts",
    "build": "vite build -c config/vite.config.ts",
    "electron:serve": "ts-node --files script/build.ts --watch",
    "electron:build": "run-s clean check build _build:background _electron:pack",
    "clean": "rimraf build dist",
    "check": "run-s typescript:check prettier:check eslint:check",
    "typescript:check": "tsc --noEmit",
    "eslint:check": "eslint \"{src,test,script,config}/**/*.{js,ts,vue}\"",
    "prettier:check": "prettier --check \"{src,test}/**/*.{ts,js,json,vue}\"",
    "prettier:fix": "prettier --write \"{src,test}/**/*.{ts,js,json,vue}\"",
    "_build:background": "ts-node --files script/build.ts",
    "_electron:pack": "electron-builder --config config/electron-builder.json",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "author": "yagisumi",
  "repository": "github:yagisumi/vite-n7zip_monitor",
  "homepage": "http://github.com/yagisumi/vite-n7zip_monitor#readme",
  "license": "MIT",
  "prettier": "@yagisumi/my-config-prettier",
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "dependencies": {
    "vue": "^3.0.2"
  },
  "devDependencies": {
    "@types/node": "^14.14.6",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "@vue/compiler-sfc": "^3.0.2",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/eslint-plugin": "^4.2.0",
    "@yagisumi/my-config-prettier": "^1.0.0",
    "autoprefixer": "^9.8.6",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "pug": "^3.0.0",
    "pug-plain-loader": "^1.0.0",
    "rimraf": "^3.0.2",
    "stylus": "^0.54.8",
    "typescript": "^4.0.5",
    "vite": "^1.0.0-rc.9"
  }
}
